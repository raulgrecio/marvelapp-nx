{
  "npmScope": "nxrgr",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "marvel-app": {
      "tags": []
    },
    "marvel-models": {
      "tags": []
    },
    "marvel-services": {
      "tags": []
    },
    "marvel-store": {
      "tags": []
    },
    "marvel-ui-app": {
      "tags": []
    }
  }
}
